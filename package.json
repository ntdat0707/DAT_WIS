  "scripts": {
    "build": "tsc",
    "build-check": "npm run build-ts && npm run lint",
    "typecheck": "rm -rf dist && tsc --noUnusedLocals",
    "lint": "tslint --project tsconfig.json -c tslint.commit.json --fix",
    "ts-start-server": "export NODE_ENV=development&&nodemon --exec ts-node -r dotenv/config ./src/run/index.ts",
    "ts-start-server-windows": "set NODE_ENV=development&&nodemon --exec ts-node -r dotenv/config ./src/run/index.ts",
    "start-docker": "docker-compose -f docker-compose.yml up --build",
    "stop-docker": "docker-compose -f ./docker-compose.yml down --rmi local",
    "ts-start-server-docker": "ts-node -r dotenv/config ./src/run/index.ts",
    "js-start-server": "node ./dist/run/index.js",
    "js-start-server-watch": "nodemon ./dist/run/server.js",
    "test": "jest --forceExit --coverage --verbose --runInBand --colors --no-env",
    "prettier": "prettier --write 'src/**/*.ts' --config ./.prettierrc"
  },
